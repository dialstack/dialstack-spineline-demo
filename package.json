{
  "name": "dialstack-spineline-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "FORCE_COLOR=1 next dev 2>&1 | pino-pretty -c -i pid,hostname -t HH:MM:ss",
    "build": "next build",
    "start": "next start",
    "start:migrate": "node scripts/migrate.mjs && next start",
    "migrate": "node --env-file=.env scripts/migrate.mjs",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@dialstack/sdk": "^0.2.2-alpha.6",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.544.0",
    "next": "^15",
    "next-auth": "^4.24.11",
    "node-pg-migrate": "^7.8.2",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.63.0",
    "shadcn": "^3",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@testcontainers/postgresql": "^11.7.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "glob": "^11.1.0"
  }
}
